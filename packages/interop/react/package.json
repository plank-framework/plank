{
  "name": "@plank/react",
  "version": "0.1.0",
  "description": "React interop for Plank framework - use React components as islands",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "clean": "rm -rf dist",
    "type-check": "tsc --noEmit",
    "lint": "biome lint .",
    "lint:fix": "biome check --write .",
    "format": "biome format .",
    "format:fix": "biome check --write .",
    "check": "biome check .",
    "check:fix": "biome check --write .",
    "test": "vitest",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:coverage:ui": "vitest run --coverage --ui"
  },
  "peerDependencies": {
    "react": "^18.0.0",
    "react-dom": "^18.0.0"
  },
  "dependencies": {
    "@plank/runtime-core": "workspace:*",
    "@plank/runtime-dom": "workspace:*"
  },
  "devDependencies": {
    "@testing-library/react": "^16.0.0",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "jsdom": "^24.1.0",
    "react": "^18.3.0",
    "react-dom": "^18.3.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "plank",
    "react",
    "interop",
    "islands",
    "web-components"
  ],
  "author": "Plank Framework",
  "license": "Apache-2.0"
}
